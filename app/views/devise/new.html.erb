<h2>Sign up</h2>

<%= form_with model: @user, url: user_session_path do |f| %>
<%= render "devise/shared/error_messages", resource: resource %>

<div class="field">
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</div>

<div class="field">
  <%= f.label :email %><br>
  <%= f.email_field :email, autocomplete: "email" %>
</div>
